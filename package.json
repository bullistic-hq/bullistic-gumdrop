{
  "name": "@formfunction-hq/formfunction-gumdrop",
  "version": "0.0.51",
  "description": "TypeScript SDK for interacting with Formfunction's gumdrop program for pNFT distribution.",
  "repository": "https://github.com/formfunction-hq/formfn-gumdrop",
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "@formfunction-hq:registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "prepare": "husky install",
    "prettier": "prettier --check \"src/**/*.ts\"",
    "eslint": "eslint --cache 'src/**/*.ts'",
    "lint": "yarn prettier --write && yarn eslint --fix",
    "test-sdk": "jest src/tests/sdk/*.test.ts",
    "test": "yarn test-sdk && ./scripts/test.sh",
    "test-fast": "./scripts/test.sh claimEditionSingleCase.test.ts",
    "test-debug": "DEBUG=true BLUEBIRD_LONG_STACK_TRACES=1 ./scripts/test.sh",
    "test-program": "jest",
    "tsc": "tsc",
    "build-sdk": "tsc -P tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "setup": "./scripts/setup.sh",
    "build-program": "anchor build && yarn setup",
    "build": "yarn build-program && yarn build-sdk",
    "deploy-program": "./scripts/deploy-program.sh",
    "set-config": "./scripts/set-config.sh",
    "set-config-authority": "ts-node -r tsconfig-paths/register src/scripts/createGumdropConfigScript.ts",
    "update-config": "./scripts/update-config.sh",
    "update-config-authority": "ts-node -r tsconfig-paths/register src/scripts/updateGumdropConfigScript.ts",
    "estimate-merkle-proof-sizes": "ts-node -r tsconfig-paths/register src/scripts/estimateMerkleProofSizes.ts",
    "modify-program-idl": "ts-node -r tsconfig-paths/register -T src/scripts/modifyProgramIdl.ts",
    "lint-program-idl": "ts-node -r tsconfig-paths/register src/scripts/lintProgramIdl.ts",
    "dump": "./scripts/solana-program-dump.sh"
  },
  "dependencies": {
    "@formfunction-hq/formfunction-program-shared": "0.0.58",
    "@solana/spl-token": "0.3.7",
    "bn.js": "^5.2.0",
    "bs58": "^5.0.0",
    "js-sha3": "^0.8.0",
    "tiny-invariant": "^1.2.0"
  },
  "peerDependencies": {
    "@project-serum/anchor": "^0.26.0",
    "@solana/web3.js": "^1.73.0"
  },
  "devDependencies": {
    "@metaplex-foundation/mpl-token-metadata": "^2.7.0",
    "@project-serum/anchor": "^0.26.0",
    "@solana/web3.js": "^1.73.0",
    "@types/bluebird": "^3.5.36",
    "@types/bn.js": "^5.1.1",
    "@types/chai": "^4.3.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "bluebird": "^3.7.2",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "husky": "^8.0.3",
    "jest": "^27.5.1",
    "npm": "^8.5.5",
    "prettier": "^2.6.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "tsc-alias": "^1.6.6",
    "typescript": "^4.9.3"
  }
}
